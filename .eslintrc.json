{
  "env": {
    "browser": true,
    "es2021": true,
    "jquery": true
  },
  "extends": [
    "standard"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "semi": ["error", "never"],
    "indent": ["off"],
    "quotes": ["error", "single"],
    "space-before-function-paren": ["off"],
    "object-curly-spacing": ["off"]
  }
}
